use conoweb;

-- Crear tabla para los proveedores
-- Name
-- Contact name
-- Phone
-- Email
-- Company Name

Create table Supplier(
    id int(11) primary key auto_increment,
    name varchar(100),
    contact_name varchar(100),
    phone varchar(100),
    email varchar(100),
    company_name varchar(100),
    created_at timestamp default current_timestamp
);


