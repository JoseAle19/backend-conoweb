Create table Users(
    id int primary key auto_increment,
    name varchar(50),
    lastname varchar(50),
    email varchar(50),
    password varchar(50),
    created_at timestamp default current_timestamp,
    rol_id int(11),
    foreign key (rol_id) references Roles(id)
);
